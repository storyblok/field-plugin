{
  "name": "@storyblok/field-plugin-cli",
  "version": "0.0.1-alpha.1",
  "type": "module",
  "bin": {
    "field-plugin": "./bin.js"
  },
  "files": [
    "bin.js",
    "dist"
  ],
  "scripts": {
    "prepare": "yarn build",
    "create": "vite build:quiet && node bin.js",
    "deploy": "vite build:quiet && node bin.js deploy",
    "build": "vite build && cp -r templates dist/",
    "build:quiet": "vite build --logLevel error",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "commander": "^9.5.0",
    "dotenv": "^16.0.3",
    "execa": "^6.1.0",
    "fs-extra": "^11.1.0",
    "kleur": "^4.1.5",
    "node-fetch": "^3.3.0",
    "prompts": "^2.4.2",
    "vite": "^4.0.3",
    "walkdir": "^0.4.1"
  },
  "devDependencies": {
    "@types/execa": "2.0.0",
    "@types/fs-extra": "11.0.1",
    "@types/prompts": "2.4.2",
    "vitest": "0.25.3"
  }
}
