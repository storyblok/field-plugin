{
  "name": "@storyblok/field-plugin",
  "version": "1.0.1",
  "description": "SDK for creating Field Plugins for Storyblok.",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./react": {
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.cjs"
    },
    "./vue3": {
      "import": "./dist/vue3/index.mjs",
      "require": "./dist/vue3/index.cjs"
    },
    "./vite": {
      "import": "./dist/vite/index.mjs",
      "require": "./dist/vite/index.cjs"
    },
    "./test": {
      "import": "./dist/test/index.mjs",
      "require": "./dist/test/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/storyblok/field-plugin.git",
    "directory": "packages/field-plugin"
  },
  "scripts": {
    "check:types": "tsc --noEmit",
    "dev": "vite build --watch",
    "test": "jest",
    "build": "unbuild && yarn build:helpers",
    "build:helpers": "cd helpers && unbuild"
  },
  "devDependencies": {
    "@types/core-js": "2.5.5",
    "@types/jest": "29.0.3",
    "@types/node": "*",
    "core-js": "3.32.1",
    "execa": "8.0.1",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.4.2",
    "ts-jest": "29.0.1",
    "typescript": "*",
    "unbuild": "2.0.0",
    "vite": "*",
    "vite-plugin-dts": "*"
  }
}
