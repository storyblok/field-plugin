{
  "name": "@storyblok/field-plugin",
  "version": "0.1-alpha",
  "description": "SDK for creating Field Plugins for Storyblok.",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "module": "./dist/field-plugin.js",
  "main": "./dist/field-plugin.umd.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/field-plugin.js",
      "require": "./dist/field-plugin.umd.cjs"
    }
  },
  "scripts": {
    "check:types": "tsc --noEmit",
    "build": "vite build && tsc --declaration --emitDeclarationOnly"
  },
  "devDependencies": {
    "@types/jest": "*",
    "@types/node": "*",
    "jest": "*",
    "ts-jest": "*",
    "typescript": "*",
    "vite": "*",
    "vite-plugin-dts": "*"
  }
}
