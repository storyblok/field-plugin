{
  "name": "@storyblok/field-plugin",
  "version": "0.0.1-beta.4",
  "description": "SDK for creating Field Plugins for Storyblok.",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "module": "./dist/field-plugin.js",
  "main": "./dist/field-plugin.umd.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/field-plugin.js",
      "require": "./dist/field-plugin.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.umd.cjs",
      "types": "./dist/react/index.d.ts"
    },
    "./vue2": {
      "import": "./dist/vue2/index.js",
      "require": "./dist/vue2/index.umd.cjs"
    },
    "./vue3": {
      "import": "./dist/vue3/index.js",
      "require": "./dist/vue3/index.umd.cjs",
      "types": "./dist/vue3/index.d.ts"
    },
    "./vite": {
      "import": "./dist/vite/index.js",
      "require": "./dist/vite/index.umd.cjs",
      "types": "./dist/vite/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/storyblok/field-plugin.git",
    "directory": "packages/field-plugin"
  },
  "scripts": {
    "check:types": "tsc --noEmit",
    "dev": "vite build --watch",
    "test": "jest",
    "build": "rm -rf dist && vite build && yarn build:helpers"
  },
  "devDependencies": {
    "@types/jest": "29.0.3",
    "@types/node": "*",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.4.2",
    "ts-jest": "29.0.1",
    "typescript": "*",
    "vite": "*",
    "vite-plugin-dts": "*"
  }
}
