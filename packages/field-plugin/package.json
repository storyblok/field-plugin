{
  "name": "@storyblok/field-plugin",
  "version": "1.7.0",
  "description": "SDK for creating Field Plugins for Storyblok.",
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./react": {
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.cjs"
    },
    "./vue3": {
      "import": "./dist/vue3/index.mjs",
      "require": "./dist/vue3/index.cjs"
    },
    "./vite": {
      "import": "./dist/vite/index.mjs",
      "require": "./dist/vite/index.cjs"
    },
    "./test": {
      "import": "./dist/test/index.mjs",
      "require": "./dist/test/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/storyblok/field-plugin.git",
    "directory": "packages/field-plugin"
  },
  "scripts": {
    "check:types": "tsc --noEmit",
    "dev": "vite build --watch",
    "test": "vitest",
    "build": "unbuild"
  },
  "devDependencies": {
    "@types/core-js": "2.5.8",
    "@types/node": "^22.17.0",
    "core-js": "3.41.0",
    "execa": "8.0.1",
    "jsdom": "25.0.0",
    "typescript": "5.8.2",
    "unbuild": "2.0.0",
    "vite": "5.4.16",
    "vite-plugin-dts": "4.2.1",
    "vitest": "2.1.9"
  },
  "dependencies": {
    "valibot": "1.1.0"
  }
}
